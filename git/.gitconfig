[init]
    defaultBranch = master
[user]
    useConfigOnly = true
    email = ynghykpark@gmail.com
    name = yonghyukpark
[pretty]
    custom = "%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)"
[alias]
    lg = log --graph --oneline --decorate --all --pretty=custom
[commit]
    verbose = true
